{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/services/*": [
        "services/*"
      ],
      "@/models/*": [
        "models/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/store/*": [
        "store/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "backup/**/*",
    "node_modules"
  ]
}
